import java.io.*;
import java.util.*;
class number
{
int x,y,z;
void display()
{
Scanner s=new Scanner(System.in);
System.out.println(&quot;enter the two numbers:&quot;);
x = s.nextInt();
y = s.nextInt();

System.out.println(&quot;after swapping:&quot;);
z=x;
x=y;
y=z;
System.out.println(x+&quot;,&quot;+y);
}
}
class swap
{
public static void main(String args[])
{
number n=new number();
n.display();
}
}
