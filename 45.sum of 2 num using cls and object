import java.io.*;
import java.util.*;
class add
{
int x,y;
void display()
{
Scanner s=new Scanner(System.in);
System.out.println(&quot;enter the two numbers:&quot;);
x = s.nextInt();

y = s.nextInt();

System.out.println(&quot;sum of two numbers is:&quot;+(x+y));
}
}
class sum
{
public static void main(String args[])
{
add a=new add();
a.display();
}
}
